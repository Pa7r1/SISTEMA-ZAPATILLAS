{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "tsx watch --clear-screen=false src/app.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "seed": "tsx src/seeder.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "typescript",
    "express",
    "typeorm",
    "zapatillas"
  ],
  "author": "",
  "license": "ISC",
  "description": "Sistema de gestiÃ³n de zapatillas con TypeScript y TypeORM",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  }
}
